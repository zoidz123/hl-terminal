{
  "name": "@zoidz123/hyperliquid",
  "version": "0.1.0",
  "description": "AI-powered terminal interface for learning and exploring Hyperliquid",
  "type": "module",
  "bin": {
    "hl": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsx src/index.tsx",
    "build": "tsup src/index.tsx --format esm --dts --target node20",
    "typecheck": "tsc --noEmit",
    "fetch-docs": "cd scripts/gitbook-downloader && poetry run python cli.py download https://hyperliquid.gitbook.io/hyperliquid-docs -o ../../docs/hyperliquid-docs.md",
    "build-prompt": "tsx scripts/build-system-prompt.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.52.0",
    "@nktkas/hyperliquid": "^0.25.0",
    "cli-highlight": "^2.1.11",
    "ink": "^5.0.1",
    "react": "^18.3.1",
    "viem": "^2.43.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.17.0",
    "@types/react": "^18.3.0",
    "tsup": "^8.3.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "hyperliquid",
    "trading",
    "defi",
    "terminal",
    "tui",
    "cli"
  ],
  "license": "MIT"
}
